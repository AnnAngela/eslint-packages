{
  "name": "@annangela/eslint-config",
  "version": "4.0.2",
  "description": "ESLint config for AnnAngela's projects",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./tsconfig.base.json": "./dist/tsconfig.base.json"
  },
  "files": [
    "./dist"
  ],
  "type": "module",
  "scripts": {
    "package": "node scripts/build.js",
    "preversion": "eslint --exit-on-fatal-error --max-warnings 0 ./src",
    "version": "npm run package",
    "tag": "node ../../scripts/newTag.js"
  },
  "engines": {
    "node": "^20.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnAngela/eslint-packages.git",
    "directory": "packages/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin"
  ],
  "author": "AnnAngela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnnAngela/eslint-packages/issues"
  },
  "homepage": "https://github.com/AnnAngela/eslint-packages/packages/eslint-config#readme",
  "devDependencies": {
    "@annangela/eslint-formatter-gha": "^1.1.0",
    "@types/node": "^20.11.1",
    "semver": "^7.5.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@annangela/eslint-plugin-prefer-reflect": "^2.2.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.1.0",
    "@eslint/js": "^8.56.0",
    "@stylistic/eslint-plugin": "^1.5.3",
    "@stylistic/eslint-plugin-migrate": "^1.5.3",
    "@types/eslint": "^8.56.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint-plugin-eslint-plugin": "^5.2.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prefer-arrow-functions": "^3.2.4",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.0",
    "globals": "^13.24.0"
  }
}
